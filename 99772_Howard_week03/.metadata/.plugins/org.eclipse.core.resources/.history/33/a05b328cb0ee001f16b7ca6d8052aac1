package LatihanLecture;

public class Latihan1 {
	public static void main(String[] args) {
		int[] randomArrayScore = new int[10];
		for(int i = 0; i < randomArrayScore.length; i++) {
			randomArrayScore[i] = (int) Math.random() * 101;
		}
		for(int Scores: randomArrayScore) {
			System.out.println(Scores);
		}
	}

}
