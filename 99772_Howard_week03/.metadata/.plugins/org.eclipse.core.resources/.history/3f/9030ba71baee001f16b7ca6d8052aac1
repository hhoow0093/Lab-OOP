package LatihanLecture;
import java.util.Scanner;
public class Latihan3 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int numberArray[][] = new int[4][3];
		for(int i = 0; i < 4; i++) {
			for(int j = 0; j < 2; j++) {
				System.out.println(String.format("masukkan angka pada numberArray pada baris %d kolum %d", i+1, j+1));
				System.out.print("nilai: ");
				int nilai = scanner.nextInt();
				numberArray[i][j] = nilai;
			}
		}
		
		System.out.println("isi dalam array adalah:");
		for(int i =0; i < 4; i++) {
			for(int j = 0; j < 2; j++) {
				System.out.print(String.format("%d ", numberArray[i][j]));
			}
			System.out.println();
		}
		
	}

}
